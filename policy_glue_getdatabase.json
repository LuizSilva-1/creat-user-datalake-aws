{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "glue:GetDatabase",
                "glue:GetDatabases",
                "glue:GetTable",
                "glue:GetTables"
            ],
            "Resource": [
                "arn:aws:glue:us-east-1:905418218028:catalog",
                "arn:aws:glue:us-east-1:905418218028:database/*",
                "arn:aws:glue:us-east-1:905418218028:table/*/*"
            ]
        }
    ]
}
